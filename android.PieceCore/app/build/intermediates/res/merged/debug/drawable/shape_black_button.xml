<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item
        android:bottom="5dp"
        android:left="2dp"
        android:right="2dp"
        android:top="5dp"
    	android:id="@+id/state">
		<selector>
		   	<item android:state_pressed="true"
		   	      android:drawable="@drawable/min_b1" />
		    <item android:state_enabled="true"
		          android:drawable="@drawable/min_b1" />
		    <item android:state_enabled="false"
		          android:drawable="@drawable/bar_gray_roundcorner_line_black" />
		</selector>
    </item>
    <item
        android:bottom="2dp"
        android:left="0dp"
        android:right="0dp"
        android:top="2dp"
        android:id="@+id/state2">
		<selector>
		    <item android:state_pressed="true">
		        <shape android:shape="rectangle">
	    			<padding android:left="0dp" android:top="0dp"
    	    	       		 android:right="0dp" android:bottom="0dp" />
		    		<corners
		                android:bottomRightRadius="4dp"
		                android:bottomLeftRadius="4dp"
		                android:topLeftRadius="4dp"
		                android:topRightRadius="4dp" />
		    		<gradient
		          			 android:angle="270"
		        			 android:startColor="#908EB7FF"
		        			 android:endColor="#908EB7FF"
		        			 android:type="linear" />
		    		<stroke
		        			 android:width="0dp"
		        			 android:color="#E85900" />
				</shape>
		    </item>
		</selector>
    </item>
</layer-list>
